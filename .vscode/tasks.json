                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc",
            "windows": {
                "command": "cmd",
                "args": [
                    "/c",
                    "(git clean -fdx build || rmdir /s /q build) & (git clean -fdx build_shared || rmdir /s /q build_shared)"
                ],
                "options": {
                    "cwd": "${workspaceFolder}"
                },
                "problemMatcher": "$msCompile"
            }
        },
